{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "redirects": [
    {
      "source": "/",
      "destination": "/cadastrarEmpresa",
      "permanent": true
    }
  ]
  }